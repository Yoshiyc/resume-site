{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"types": [
			"svelte"
		],
		"moduleResolution": "bundler",
		"module": "ESNext",
		"target": "ESNext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"verbatimModuleSyntax": true
	},
	"include": [
		"src"
	]
}